func (this {1:StorageName}DB) GetKey() (key storage.Key) {
	return nil
}

func (this *{1:StorageName}DB) parseKey(key storage.Key) (id int64, err error) {
	tokens := strings.Split(key.String(), "|")
	if len(tokens) != 1 {
		err = errors.New("key format error:" + key.String())
		return
	}
	id, err = int64(strconv.Atoi(tokens[0]))
	if err != nil {
		return
	}
	return
}

